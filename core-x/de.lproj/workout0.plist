<plist version="1.0">
<dict>
	<key>workoutName</key>
	<string>Core X</string>
	<key>workoutDescription</key>
	<string>Core X ist eine einfache Routine - 10 Übungen, jeweils 30 Sekunden Übungen; 5 Minuten insgesamt. Während es kurz und süß ist, ist es auch sehr anspruchsvoll.</string>
	<key>defaultRestDuration</key>
	<integer>5</integer>
	<key>defaultExerciseDuration</key>
	<integer>30</integer>
	<key>exercises</key>
	<dict>
		<key>Item 0</key>
		<dict>
			<key>itemName</key>
			<string>Laufende V-Sitzt</string>
			<key>itemDescription</key>
			<string>Setzen Sie sich in einer V-Formation, und bewegen Sie die Arme in einem Sprintbewegung</string>
			<key>itemImage</key>
			<string>workout0-0.png</string>
		</dict>
		<key>Item 1</key>
		<dict>
			<key>itemName</key>
			<string>Land Baden</string>
			<key>itemDescription</key>
			<string>Bewegen Sie Ihre Arme &amp; Beine, als wenn Sie versuchen, auf dem Land, während auf dem Bauch schwimmen werden</string>
			<key>itemImage</key>
			<string>workout0-1.png</string>
		</dict>
		<key>Item 2</key>
		<dict>
			<key>itemName</key>
			<string>Flatter Tritte</string>
			<key>itemDescription</key>
