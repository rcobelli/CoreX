<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>workoutName</key>
	<string>Coach Hannah Core Routine</string>
	<key>workoutDescription</key>
	<string>A great core workout</string>
	<key>defaultRestDuration</key>
	<integer>5</integer>
	<key>defaultExerciseDuration</key>
	<integer>45</integer>
	<key>exercises</key>
	<dict>
		<key>Item 0</key>
		<dict>
			<key>itemName</key>
			<string>Plank</string>
			<key>itemDescription</key>
			<string>Hold your torso off the ground using only your feet and forearms/elbows</string>
			<key>itemImage</key>
			<string>workout6-0.png</string>
		</dict>
		<key>Item 1</key>
		<dict>
			<key>itemName</key>
			<string>Left Side Plank</string>
			<key>itemDescription</key>
			<string>Hold your torso off the ground using only your left foot and forearm/elbow</string>
			<key>itemImage</key>
			<string>workout6-1.png</string>
		</dict>
		<key>Item 2</key>
		<dict>
			<key>itemName</key>
			<string>Right Side Plank</string>
			<key>itemDescription</key>
			<string>Hold your torso off the ground using only your right foot and forearm/elbow</string>
			<key>itemImage</key>
			<string>workout6-2.png</string>
		</dict>
		<key>Item 3</key>
		<dict>
			<key>itemName</key>
			<string>V-Sit</string>
			<key>itemDescription</key>
			<string>Sit with your legs and back straight, forming a V pose</string>
			<key>itemImage</key>
			<string>workout6-3.png</string>
		</dict>
		<key>Item 4</key>
		<dict>
			<key>itemName</key>
			<string>Russian Twists</string>
			<key>itemDescription</key>
			<string>Hold a V pose but twist your abdomen back and forth (optionally with a weight)</string>
			<key>itemImage</key>
			<string>workout6-4.png</string>
		</dict>
		<key>Item 5</key>
		<dict>
			<key>itemName</key>
			<string>Sit Ups</string>
			<key>itemDescription</key>
			<string>Lay on your back and raise your torso into a sitting position without using arms or moving legs</string>
			<key>itemImage</key>
			<string>workout6-5.png</string>
		</dict>
		<key>Item 6</key>
		<dict>
			<key>itemName</key>
			<string>Leg Lifts</string>
			<key>itemDescription</key>
			<string>Lay flat on the ground and lift your legs into the air 90 degrees while keeping them straight</string>
			<key>itemImage</key>
			<string>workout6-6.png</string>
		</dict>
	</dict>
</dict>
</plist>
