<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>workoutName</key>
	<string>Core X</string>
	<key>workoutDescription</key>
	<string>Core X un simple routine - 10 exercices, 30 secondes chacun des exercices; 5 minutes au total. Alors qu'il est court et doux, il est aussi très difficile.</string>
	<key>defaultRestDuration</key>
	<integer>5</integer>
	<key>defaultExerciseDuration</key>
	<integer>30</integer>
	<key>exercises</key>
	<dict>
		<key>Item 0</key>
		<dict>
			<key>itemName</key>
			<string>Courir V-assied</string>
			<key>itemDescription</key>
			<string>Asseyez-vous dans une formation en V et bouger vos bras dans un mouvement de sprint</string>
			<key>itemImage</key>
			<string>workout0-0.png</string>
		</dict>
		<key>Item 1</key>
		<dict>
			<key>itemName</key>
			<string>Terrain piscine</string>
			<key>itemDescription</key>
			<string>Déplacez vos bras &amp; jambes comme si vous essayez de nager sur la terre tandis que sur votre estomach</string>
			<key>itemImage</key>
			<string>workout0-1.png</string>
		</dict>
		<key>Item 2</key>
		<dict>
			<key>itemName</key>
			<string>Battements de jambes</string>
			<key>itemDescription</key>
			<string>Asseyez-vous dans une formation en V et bouger vos jambes de haut en bas</string>
			<key>itemImage</key>
			<string>workout0-2.png</string>
		</dict>
		<key>Item 3</key>
		<dict>
			<key>itemName</key>
			<string>Side Plank Levées de jambe (gauche)</string>
			<key>itemDescription</key>
			<string>Tout en faisant une planche de côté, faire des levées de jambe</string>
			<key>itemImage</key>
			<string>workout0-3.png</string>
		</dict>
		<key>Item 4</key>
		<dict>
			<key>itemName</key>
			<string>Supine Plank Levées de jambe</string>
			<key>itemDescription</key>
			<string>Tout en faisant un décubitus dorsal (sur le dos) planche, faire des levées de jambe</string>
			<key>itemImage</key>
			<string>workout0-4.png</string>
		</dict>
		<key>Item 5</key>
		<dict>
			<key>itemName</key>
			<string>Side Plank Levées de jambe (droite)</string>
			<key>itemDescription</key>
			<string>Tout en faisant une planche de côté, faire des levées de jambe. Cette fois-ci avec l'autre côté</string>
			<key>itemImage</key>
			<string>workout0-5.png</string>
		</dict>
		<key>Item 6</key>
		<dict>
			<key>itemName</key>
			<string>Superman</string>
			<key>itemDescription</key>
			<string>Allongez-vous sur votre estomac et laissé vos bras / jambes pendant 2 secondes, puis se reposer pendant 1. Rincer et répéter</string>
			<key>itemImage</key>
			<string>workout0-6.png</string>
		</dict>
		<key>Item 7</key>
		<dict>
			<key>itemName</key>
			<string>Pushup de planche de côté</string>
			<key>itemDescription</key>
			<string>Faire des pompes, mais quand vous venez, pivot à une planche de commutation côté côtés à chaque fois</string>
			<key>itemImage</key>
			<string>workout0-7.png</string>
		</dict>
		<key>Item 8</key>
		<dict>
			<key>itemName</key>
			<string>Coups de ciseaux</string>
			<key>itemDescription</key>
			<string>Asseyez-vous dans une formation en V et déplacez votre côté des jambes à l'autre</string>
			<key>itemImage</key>
			<string>workout0-8.png</string>
		</dict>
		<key>Item 9</key>
		<dict>
			<key>itemName</key>
			<string>Rockies</string>
			<key>itemDescription</key>
			<string>Pompes de Clap</string>
			<key>itemImage</key>
			<string>workout0-9.png</string>
		</dict>
	</dict>
</dict>
</plist>
